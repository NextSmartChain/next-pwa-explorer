 },\n    },\n\n    mounted() {\n        document.body.appendChild(this.$el);\n\n        if (this.visible) {\n            this.$nextTick(() => {\n                this.show();\n            });\n        }\n    },\n\n    destroyed() {\n        this.$el.remove();\n    },\n\n    methods: {\n        show() {\n            if (!this.isVisible) {\n                this.isVisible = true;\n            }\n        },\n\n        hide(_hiddenByClick = false) {\n            if (this.isVisible) {\n                this.isVisible = false;\n\n                this.$nextTick(() => {\n                    this.$emit('overlay-hide', _hiddenByClick);\n                });\n            }\n        },\n\n        onClick() {\n            if (this.hideOnClick) {\n                this.hide(true);\n            }\n        },\n    },\n};\n</script>\n\n<style lang=\"scss\">\n@import 'style';\n</style>\n"]}]}