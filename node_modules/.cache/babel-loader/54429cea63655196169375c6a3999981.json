// value to format\n            value: {\n                type: Boolean,\n                default: false,\n                required: true\n            },\n            // 'yes' translation\n            yes: {\n                type: String,\n                default: ''\n            },\n            // 'no' translation\n            no: {\n                type: String,\n                default: ''\n            },\n            // style text\n            useColors: {\n                type: Boolean,\n                default: false\n            }\n        },\n\n        data() {\n            return {\n                dYes: this.yes || this.$t('yes'),\n                dNo: this.no || this.$t('no')\n            }\n        },\n\n        computed: {\n            cClass() {\n                return {\n                    yes: this.value,\n                    no: !this.value,\n                    'use-colors': this.useColors\n                }\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\">\n    .f-yes-no {\n        &.use-colors {\n            &.yes {\n                color: $success-color;\n            }\n\n            &.no {\n                color: $error-color;\n            }\n        }\n    }\n</style>\n"]}]}